<Window x:Class="simsProj.Gui.ClanGui.View.ClanWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:simsProj.Gui.ClanGui.View"
        mc:Ignorable="d"
        Title="ClanWindow" Height="450" Width="800">
    <Grid Background="#FFE5E5E5">
        <Button Content="Iznajmi knjige" HorizontalAlignment="Left" Margin="254,75,0,0" VerticalAlignment="Top" Height="50" Width="284"  FontFamily="Lucida Calligraphy" FontSize="14" Background="White" Command="{Binding IznajmiCommand}"/>
        <Button Content="Vrati knjige" HorizontalAlignment="Left" Margin="254,188,0,0" VerticalAlignment="Top" Height="50" Width="284"  FontFamily="Lucida Calligraphy" FontSize="14" Background="White" Command="{Binding VratiCommand}"/>

    </Grid>
</Window>
